  <section class="py-6 section-striped">
		<div class="container">
			<div class="row">
				<div class="col-xl-8 offset-xl-2">
          <h1><span property="schema:name" class="field field--name-title field--type-string field--label-hidden">کتب رهنمای معلم</span>
</h1>
          <div class="lead">
            
            
        <h2>مواد آموزشی تربیه معلم</h2>

<table border="1" cellpadding="0" cellspacing="0">
	<thead>
		<tr>
			<th scope="col" style="width: 26px;">شماره</th>
			<th scope="col" style="width: 629px;">عنوان</th>
			<th scope="col">نوع کتاب</th>
			<th scope="col">دانلود</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td style="width: 26px;">۱</td>
